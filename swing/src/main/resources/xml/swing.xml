<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE swings SYSTEM "../dtd/swing.dtd">

<swings>
    <swing id="buy" validateType="buyPrice" expression="10">
        <swing id="buyCount" validateType="buyCountPercent" expression="10#0.2" orElse="defaultTrue">
            <swing id="continueUp" validateType="continuedUp" expression="2#3" executor="org.eddy.swing.solver.LogFlowSolver"/>
        </swing>
    </swing>
    <swing id="defaultTrue" validateType="defaultValidater" executor="org.eddy.swing.solver.ElseFlowSolver"/>
</swings>